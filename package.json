{
  "name": "youtube-auto-hd-fps",
  "version": "1.4",
  "homepage": "https://avi12.com",
  "repository": "https://github.com/avi12/youtube-auto-hd",
  "author": "avi12 <avi6106@gmail.com>",
  "bugs": {
    "email": "avi6106@gmail.com"
  },
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "pack": "web-ext build --overwrite-dest --artifacts-dir dist_packed --source-dir dist --filename youtube_auto_hd_fps-{version}.zip",
    "pack-stores": "webext-store-incompat-fixer -i dist_packed/youtube_auto_hd_fps-{version}.zip --edge-locale-inclusions en,sr,hr,de,de_CH,de_AT,es,es_419,es_US,fr,fr_CA,he,ja,nl,pl,pt_BR,ru,zh_Hans,zh_Hant --stores chrome,edge,firefox,opera",
    "pack-self": "zip-self -o dist_packed/youtube_auto_hd_fps-{version}-source.zip",
    "build-pack": "pnpm run build && pnpm run pack && pnpm run pack-stores && pnpm run pack-self",
    "build-for-firefox": "build-for-browser --i dist_packed/youtube_auto_hd_fps-{version}.zip --browser=firefox",
    "build-for-opera": "build-for-browser --i dist_packed/youtube_auto_hd_fps-{version}.zip --browser=opera",
    "set-version": "set-json-version -f package.json dist/manifest.json -v",
    "validate": "svelte-check",
    "run-browser": "web-ext run --no-config-discovery --start-url https://www.youtube.com/watch?v=dQw4w9WgXcQ --source-dir dist --verbose --target",
    "run-firefox": "pnpm run-browser firefox-desktop",
    "run-chromium": "pnpm run-browser chromium",
    "run-edge-windows": "\"\"%ProgramFiles(x86)%\\Microsoft\\Edge\\Application\\msedge.exe\"\" https://www.youtube.com/watch?v=dQw4w9WgXcQ --load-extension=%cd%\\dist"
  },
  "devDependencies": {
    "@mdi/js": "^6.5.95",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "~13.1.2",
    "@rollup/plugin-typescript": "^8.3.0",
    "@sveltejs/eslint-config": "github:sveltejs/eslint-config",
    "@tsconfig/svelte": "^3.0.0",
    "@types/chrome": "^0.0.174",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "build-for-browser": "github:avi12/build-for-browser",
    "eslint": "^8.6.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.1",
    "rollup": "~2.62.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-svelte": "~7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.45.2",
    "set-json-version": "^1.0.7",
    "stylelint": "^14.2.0",
    "stylelint-config-ccb": "^1.6.5",
    "svelte": "~3.44.3",
    "svelte-check": "^2.2.11",
    "svelte-materialify": "^0.3.11",
    "svelte-preprocess": "^4.10.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "web-ext": "^6.6.0",
    "webext-store-incompat-fixer": "^0.1.39",
    "zip-self": "^0.4.5"
  }
}
