{
  "name": "youtube-auto-hd",
  "displayName": "YouTube Auto HD + FPS",
  "version": "1.8.2",
  "description": "Automatically set the video quality on YouTube according to its FPS!",
  "author": "avi12 <avi6106@gmail.com>",
  "homepage": "https://github.com/avi12/youtube-auto-hd",
  "scripts": {
    "dev": "plasmo dev --verbose",
    "dev:firefox": "plasmo dev --verbose --target=firefox-mv2",
    "build": "plasmo build --minify",
    "build:firefox": "plasmo build --minify --target=firefox-mv2",
    "package-test": "plasmo package",
    "package": "web-ext build --source-dir build/chrome-mv3-prod --filename youtube-auto-hd-fps-{version}.zip",
    "package:self": "zip-self -o web-ext-artifacts/youtube-auto-hd-fps-{version}-source.zip",
    "package:stores": "webext-store-incompat-fixer -i web-ext-artifacts/youtube-auto-hd-fps-{version}.zip --edge-locale-inclusions en,fr,he,pt_BR,be,de,nl,pl,ru,zh,zh_CN,zh_TW --stores chrome,edge,opera",
    "package:stores-firefox": "webext-store-incompat-fixer -i web-ext-artifacts/youtube-auto-hd-fps-{version}_adapted_for_firefox.zip --stores firefox",
    "package:firefox": "web-ext build --source-dir build/firefox-mv2-prod --filename youtube-auto-hd-fps-{version}_adapted_for_firefox.zip && pnpm package:stores-firefox",
    "build-pack": "pnpm build && pnpm package && pnpm package:stores",
    "build-pack:test": "pnpm build && pnpm package-test",
    "build-pack:test-push": "cmd /V /C \"set zip=chrome-mv3-prod.zip & set dest_android=/storage/emulated/0/Download/!zip! & pnpm build-pack:test & adb shell rm !dest_android! & adb push build/!zip! !dest_android!\"",
    "build-pack:firefox": "pnpm build:firefox && pnpm package:firefox && pnpm package:self",
    "build-pack:all": "pnpm build-pack && pnpm build-pack:firefox",
    "run-browser": "web-ext run --start-url https://www.youtube.com/watch?v=aiSla-5xq3w --target",
    "run-firefox": "pnpm run-browser firefox-desktop --source-dir build/firefox-mv2-dev",
    "run-chromium": "pnpm run-browser chromium --source-dir build/chrome-mv3-dev",
    "run-chromium:rtl": "pnpm run-browser chromium --source-dir build/chrome-mv3-dev --args=\"--lang=he\"",
    "run-edge:windows": "pnpm run-browser chromium --source-dir build/chrome-mv3-dev --chromium-binary \"%ProgramFiles(x86)%\\Microsoft\\Edge\\Application\\msedge.exe\"",
    "run-opera:windows": "pnpm run-browser chromium --source-dir build/chrome-mv3-dev --chromium-binary \"%LocalAppData%\\Programs\\Opera\\opera.exe\"",
    "run-browser:screenshot": "web-ext run --start-url about:blank --target",
    "run-firefox:screenshot": "pnpm run-browser:screenshot firefox-desktop --source-dir build/firefox-mv2-prod",
    "run-chromium:screenshot": "pnpm run-browser:screenshot chromium --source-dir build/chrome-mv3-dev",
    "run-opera:screenshot": "pnpm run-browser:screenshot chromium --source-dir build/chrome-mv3-prod --chromium-binary \"%LocalAppData%\\Programs\\Opera\\opera.exe\"",
    "run-edge:windows-embed": "web-ext run --start-url file:///%cd%/tests/local-embed.html --source-dir build/chrome-mv3-dev --target chromium --chromium-binary \"%ProgramFiles(x86)%\\Microsoft\\Edge\\Application\\msedge.exe\""
  },
  "dependencies": {
    "@mdi/js": "^7.2.96",
    "@plasmohq/storage": "^1.7.2",
    "nouislider": "^15.7.1",
    "plasmo": "0.82.0",
    "sass": "1.64.1",
    "svelte": "4.1.1",
    "svelte-preprocess": "5.0.4"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "4.0.1",
    "@sveltejs/eslint-config": "github:sveltejs/eslint-config",
    "@types/chrome": "0.0.242",
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.46.0",
    "eslint-config-avi12": "github:avi12/eslint-config-avi12",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.27",
    "prettier": "3.0.0",
    "prettier-plugin-svelte": "^3.0.3",
    "tailwindcss": "^3.3.3",
    "tailwindcss-rtl": "^0.9.0",
    "typescript": "5.1.6",
    "web-ext": "^7.6.2",
    "webext-store-incompat-fixer": "^0.4.2",
    "zip-self": "^0.5.9"
  },
  "manifest": {
    "description": "__MSG_cj_i18n_02146__",
    "default_locale": "en",
    "host_permissions": [
      "https://youtube.com/*",
      "https://*.youtube.com/*",
      "https://www.youtube-nocookie.com/*",
      "https://youtube.googleapis.com/*"
    ],
    "browser_specific_settings": {
      "gecko": {
        "id": "$PLASMO_PUBLIC_FIREFOX_EXT_ID",
        "strict_min_version": "90.0"
      }
    },
    "options_ui": {
      "browser_style": true,
      "open_in_tab": false
    },
    "permissions": [
      "cookies"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "assets/*"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ]
  }
}
